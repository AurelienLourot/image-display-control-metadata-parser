{
  "name": "@frameright/image-display-control-metadata-parser",
  "description": "Image Display Control metadata parsing library",
  "keywords": [
    "metadata",
    "image",
    "image-manipulation",
    "responsive-design",
    "responsive-layout",
    "responsive-images",
    "metadata-extraction",
    "metadata-parser",
    "iptc-metadata",
    "image-display-control",
    "frameright",
    "image-publishing"
  ],
  "version": "1.1.1",
  "license": "MIT",
  "repository": "https://github.com/Frameright/image-display-control-metadata-parser",
  "author": {
    "name": "Frameright (Coberg Ltd)",
    "email": "hello@frameright.io",
    "url": "https://frameright.io/"
  },
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./dist/image-display-control-metadata-parser-standalone.min.js": "./dist/image-display-control-metadata-parser-standalone.min.js"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && rollup -c",
    "test": "tsdx test --no-cache",
    "lint": "eslint . --ignore-pattern dist && prettier --check .",
    "format": "eslint . --ignore-pattern dist --fix && prettier --write .",
    "prepare": "husky install && tsdx build && rollup -c",
    "gendoc": "typedoc",
    "gentoc": "./bin/gentoc.sh"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/image-display-control-metadata-parser.esm.js",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@web/rollup-plugin-copy": "^0.4.0",
    "husky": "^8.0.3",
    "markdown-toc": "^1.2.0",
    "prettier": "^3.0.0",
    "rollup": "^3.20.7",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-summary": "^2.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.15.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "exifreader": "^4.12.0",
    "image-size": "^1.0.2"
  },
  "_FIXME": "most overrides are caused by tsdx not being maintained anymore. See https://github.com/Frameright/image-display-control-metadata-parser/issues/5",
  "overrides": {
    "eslint": "^8.46.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.0"
  }
}
