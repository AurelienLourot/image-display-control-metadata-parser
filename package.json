{
  "name": "@frameright/image-display-control-metadata-parser",
  "description": "Image Display Control metadata parsing library",
  "keywords": [
    "metadata",
    "image",
    "image-manipulation",
    "responsive-design",
    "responsive-layout",
    "responsive-images",
    "metadata-extraction",
    "metadata-parser",
    "iptc-metadata",
    "image-display-control",
    "frameright",
    "image-publishing"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Frameright (Coberg Ltd)",
    "email": "hello@frameright.io",
    "url": "https://frameright.io/"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && rollup -c",
    "test": "tsdx test --no-cache",
    "lint": "tsdx lint . --ignore-pattern dist && prettier --check .",
    "format": "tsdx lint . --ignore-pattern dist --fix && prettier --write .",
    "prepare": "husky install && tsdx build && rollup -c",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "gendoc": "typedoc",
    "gentoc": "./bin/gentoc.sh",
    "spellcheck": "npx mdspell '**/*.md' '!**/node_modules/**' '!**/generated*/**' --ignore-acronyms --ignore-numbers --no-suggestions --en-us"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/image-display-control-metadata-parser.esm.js",
  "size-limit": [
    {
      "path": "dist/image-display-control-metadata-parser.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/image-display-control-metadata-parser.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@web/rollup-plugin-copy": "^0.4.0",
    "husky": "^8.0.3",
    "markdown-spellcheck": "^1.3.1",
    "markdown-toc": "^1.2.0",
    "prettier": "^2.8.7",
    "rollup": "^2.79.1",
    "rollup-plugin-summary": "^2.0.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.24.4",
    "typedoc-plugin-markdown": "^3.15.1",
    "typescript": "^4.1.6"
  },
  "dependencies": {
    "exifreader": "^4.12.0",
    "image-size": "^1.0.2"
  }
}
