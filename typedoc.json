{
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-remark"],
  "remarkPlugins": [["remark-toc", { "maxDepth": 3 }]],
  "entryPoints": ["./src/index.ts"],
  "out": "generated-docs/",
  "readme": "none",
  "sort": ["source-order"],
  "excludePrivate": true,
  "excludeNotDocumented": true,
  "excludeNotDocumentedKinds": ["Constructor", "ConstructorSignature"],
  "gitRevision": "main"
}
